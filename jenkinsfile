pipeline {
    agent any 
    tools{
        maven 'Maven'
    }
    stages{
        stage("Building stage") {
            steps{
                echo "Building Application..."
                sh "mvn clean package"
            }
        }

        stage("Testing stage") {
            steps{
                echo "Testing Application"
            }
        
        }

        stage("Deploy stage") {
            steps{
                echo "Deploy Application"
            }
        
        }

     }
}
    
